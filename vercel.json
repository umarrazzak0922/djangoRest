{
    "builds": [{
        "src": "testproject/asgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "@vercel/python" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "testproject/asgi.py"
        }
    ]
}